{
  "name": "e_commerce_nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --no-cache",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,css,scss}\"",
    "format:fix": "prettier --write --ignore-path .gitignore .",
