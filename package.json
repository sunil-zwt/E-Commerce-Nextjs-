    "@typescript-eslint/parser": "^7.7.0",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jquery": "^3.7.1",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
